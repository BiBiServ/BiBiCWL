    <ns4:executable>
        <ns4:version>0.1</ns4:version>
        <ns4:execInfo>
            <ns4:executableType>docker</ns4:executableType>
            <ns4:image>quay.io/biocontainers/qiime:1.9.1--np112py27_1</ns4:image>
            <ns4:path></ns4:path>
            <!--
                The "add-metadata" part of the command is put into an additionalString
                in the paramsAndInputOuputOrder.
            -->
            <ns4:callingInformation>biom</ns4:callingInformation>
        </ns4:execInfo>
        
        
        <ns4:input id="input_out_table_mc2_w_tax_no_pynast_failures_biom">
            <ns4:name>QIIME open reference OTU biom data</ns4:name>
            <ns4:shortDescription>Open reference OTU biom data from QIIME's pick open reference OTUs function Input for QIIME</ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>FILE</ns4:handling>
            <ns4:option>-i </ns4:option>
        </ns4:input>
        <ns4:input id="input_metadata_mapping_files">
            <ns4:name>Sample metadata</ns4:name>
            <ns4:shortDescription>Metadata of the sample</ns4:shortDescription>
            <ns4:description>
                The sample metadata mapping file (will add
                sample metadata to the input BIOM table, if
                provided).
            </ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>FILE</ns4:handling>
            <ns4:option>-m </ns4:option>
        </ns4:input>
        
        
        <ns4:output id="output_biom">
            <ns4:name>Biom ouput</ns4:name>
            <ns4:shortDescription>Output of QIIME's Biom tool.</ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>FILE</ns4:handling>
            <ns4:option>-o </ns4:option>
        </ns4:output>
        <ns4:output id="PLACEHOLDER_output_biom">
            <ns4:name>Placeholder output</ns4:name>
            <ns4:shortDescription></ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>DIRECTORY</ns4:handling>
        </ns4:output>
        
        
        <ns4:outputfile id="outputfile_out_table_mc2_w_tax_no_pynast_failures_metadata_biom">
            <ns4:name>out_table_mc2_w_tax_no_pynast_failures_metadata_biom</ns4:name>
            <ns4:filename>out_table_mc2_w_tax_no_pynast_failures_metadata.biom</ns4:filename>
            <ns4:contenttype>text/plain</ns4:contenttype>
        </ns4:outputfile>
        
        
        <!--ns4:param id="sample_metadata">
            <ns4:name>Sample metadata</ns4:name>
            <ns4:shortDescription>File with Metadata of the sample.</ns4:shortDescription>
            <ns4:description>
                The sample metadata mapping file (will add
                  sample metadata to the input BIOM table, if
                  provided).
            </ns4:description>
            <ns4:type>string</ns4:type>
            <ns4:option>-m </ns4:option>
            <ns4:guiElement>INPUTTEXT</ns4:guiElement>
        </ns4:param-->
        
        
        <ns4:function id="function_qiime_biom_add_metadata">
            <ns4:shortDescription>Performs a fast quality control on the FASTQ file</ns4:shortDescription>
            
            <ns4:inputref ref="input_out_table_mc2_w_tax_no_pynast_failures_biom"/>
            <ns4:inputref ref="input_metadata_mapping_files"/>
            <ns4:outputref ref="output_biom"/>
            
<!--            <ns4:outputfileref ref="outputfile_out_table_mc2_w_tax_no_pynast_failures_metadata_biom"/>-->
            
            <ns4:paramAndInputOutputOrder>
                <!--
                    The "add-metadata" part of the actual command needs to
                    be added here because cwl puts single-quotes around the
                    "command" which can't be deactivated as far as I know.
                    However, quotes can be disabled for inputs, outputs and 
                    arguments. 
                -->
                <ns4:additionalString>add-metadata</ns4:additionalString>
                <ns4:reference>input_out_table_mc2_w_tax_no_pynast_failures_biom</ns4:reference>
                <ns4:reference>input_metadata_mapping_files</ns4:reference>
                <!-- 
                     the latter two are for the case that the placeholder output 
                     is used. Otherwise use the other output ... 
                -->
                <!--ns4:additionalString>-o /var/spool/cwl</ns4:additionalString-->
                <!--ns4:additionalString>-o $HOME</ns4:additionalString-->
                <ns4:reference>output_biom</ns4:reference>
            </ns4:paramAndInputOutputOrder>
        </ns4:function>
        
        
    </ns4:executable>
    
    
    
    
    <ns4:view type="submission" id="view_submission">
        <ns4:title>QIIME demultiplex submission</ns4:title>
        <ns4:customContent xml:lang="en"/>
    </ns4:view>
    
    <ns4:view type="webservice" id="view_webservice">
        <ns4:title xml:lang="en">QIIME demultiplex WebService</ns4:title>
        <ns4:customContent xml:lang="en"/>
    </ns4:view>
    
    
    
</ns4:runnableItem>
