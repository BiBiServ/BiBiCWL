<?xml version="1.0" encoding="UTF-8"?>
<ns4:runnableItem 
    xmlns:ns2="bibiserv:de.unibi.techfak.bibiserv.cms.microhtml" 
    xmlns:ns3="bibiserv:de.unibi.techfak.bibiserv.cms.minihtml" 
    xmlns:ns4="bibiserv:de.unibi.techfak.bibiserv.cms" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    id="id_of_example_tool"
    xsi:schemaLocation="bibiserv:de.unibi.techfak.bibiserv.cms http://bibiserv.techfak.uni-bielefeld.de/xsd/bibiserv2/BiBiServAbstraction.xsd">
    
    
    <ns4:name>Example Tool</ns4:name>
    <ns4:shortDescription></ns4:shortDescription>
    <ns4:description></ns4:description>
    
    
    <ns4:responsibleAuthor>
        <ns4:firstname>John</ns4:firstname>
        <ns4:lastname>Doe</ns4:lastname>
        <ns4:email>john.doe@abc.de</ns4:email>
    </ns4:responsibleAuthor>
    
    
    <ns4:executable>
        
        <ns4:version></ns4:version>
        <ns4:execInfo>
            <ns4:callingInformation></ns4:callingInformation>
        </ns4:execInfo>
        
        <ns4:input id="id_of_input">
            <ns4:name></ns4:name>
            <ns4:shortDescription></ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type></ns4:type>
            <ns4:handling></ns4:handling>
        </ns4:input>
        
        <ns4:output id="id_of_output">
            <ns4:name></ns4:name>
            <ns4:shortDescription></ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type></ns4:type>
            <ns4:handling></ns4:handling>
        </ns4:output>
        
        <ns4:function id="id_of_function">
            <ns4:shortDescription></ns4:shortDescription>
            <ns4:outputref ref="id_of_output"/>
            <ns4:paramAndInputOutputOrder></ns4:paramAndInputOutputOrder>
        </ns4:function>
        
    </ns4:executable>
    
    
    <ns4:view type="submission" id="id_view"></ns4:view>
    
</ns4:runnableItem>