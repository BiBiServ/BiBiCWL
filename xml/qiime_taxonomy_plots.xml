<?xml version="1.0" encoding="UTF-8"?>
<ns4:runnableItem 
    xmlns:ns2="bibiserv:de.unibi.techfak.bibiserv.cms.microhtml" 
    xmlns:ns3="bibiserv:de.unibi.techfak.bibiserv.cms.minihtml" 
    xmlns:ns4="bibiserv:de.unibi.techfak.bibiserv.cms" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    id="qiime_taxonomy_plots"
    xsi:schemaLocation="bibiserv:de.unibi.techfak.bibiserv.cms http://bibiserv.techfak.uni-bielefeld.de/xsd/bibiserv2/BiBiServAbstraction.xsd">
    
    
    
    
    <ns4:name>QIIME summarzize taxa through plots</ns4:name>
    <ns4:shortDescription></ns4:shortDescription>
    <ns4:description></ns4:description>
    
    
    
    
    <!-- 
        TODO!!!!
        
        QIIME requires a lot of credit-stuff. there are a dozen people who
        worked on QIIME and additionally QIIME wraps other tools which also 
        have to be cited!!!
        
        /TODO!!!!
    -->
    <ns4:responsibleAuthor>
        <ns4:firstname></ns4:firstname>
        <ns4:lastname></ns4:lastname>
        <ns4:email></ns4:email>
    </ns4:responsibleAuthor>
    
    
    
    
    <ns4:executable>
        <ns4:version>0.1</ns4:version>
        <ns4:execInfo>
            <ns4:executableType>docker</ns4:executableType>
            <ns4:image>quay.io/biocontainers/qiime</ns4:image>
            <ns4:path></ns4:path>
            <ns4:callingInformation>summarize_taxa_through_plots.py</ns4:callingInformation>
        </ns4:execInfo>
        
        
        <ns4:input id="input_otu_table">
            <ns4:name>OTU table</ns4:name>
            <ns4:shortDescription>OTU table for computing plots</ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>FILE</ns4:handling>
            <ns4:option>-i </ns4:option>
        </ns4:input>
        <ns4:input id="input_mapping">
            <ns4:name>Mapping</ns4:name>
            <ns4:shortDescription>path to mapping file</ns4:shortDescription>
            <ns4:description>
                path to the mapping file [REQUIRED if passing -c]
            </ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>FILE</ns4:handling>
            <ns4:option>-m </ns4:option>
        </ns4:input>
        <ns4:input id="input_parameter_file">
            <ns4:name>Parameter file</ns4:name>
            <ns4:shortDescription>path to parameter file</ns4:shortDescription>
            <ns4:description>
                path to the parameter file, which specifies changes to
                the default behavior. See
                http://www.qiime.org/documentation/file_formats.html#qiime-parameters . 
                [if omitted, default values will be used]
            </ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>FILE</ns4:handling>
        </ns4:input>
        
        
        <ns4:output id="output_taxa_plots">
            <ns4:name>QIIME ouput</ns4:name>
            <ns4:shortDescription>Output of QIIME.</ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>FILE</ns4:handling>
            <ns4:option>-o </ns4:option>
        </ns4:output>
        
        
        <ns4:function id="function_qiime_taxonomy_plots">
            <ns4:name>Compute beta diversity</ns4:name>
            <ns4:shortDescription></ns4:shortDescription>
            <ns4:inputref ref="input_mapping"/>
            <ns4:inputref ref="input_otu_table"/>
            <ns4:inputref ref="input_parameter_file"/>
            <ns4:outputref ref="output_taxa_plots"/>
            
            <ns4:paramAndInputOutputOrder>
                <ns4:reference>input_otu_table</ns4:reference>
                <ns4:reference>output_taxa_plots</ns4:reference>
                <ns4:reference>input_mapping</ns4:reference>
                <ns4:reference>input_parameter_file</ns4:reference>
            </ns4:paramAndInputOutputOrder>
        </ns4:function>
        
        
    </ns4:executable>
    
    
    
    
    <ns4:view type="submission" id="view_submission">
        <ns4:title>QIIME taxa plots Submission</ns4:title>
        <ns4:customContent xml:lang="en"/>
    </ns4:view>
    
    <ns4:view type="webservice" id="view_webservice">
        <ns4:title xml:lang="en">QIIME taxa plots WebService</ns4:title>
        <ns4:customContent xml:lang="en"/>
    </ns4:view>
    
    
    
</ns4:runnableItem>