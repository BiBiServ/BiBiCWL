    <ns4:executable>
        <ns4:version>0.1</ns4:version>
        <ns4:execInfo>
            <ns4:executableType>docker</ns4:executableType>
            <ns4:image>jsaydo/qiime_matplotlib_backend_agg:latest</ns4:image>
            <ns4:path></ns4:path>
            <ns4:callingInformation>pick_open_reference_otus.py</ns4:callingInformation>
        </ns4:execInfo>
        
        
        <ns4:input id="input_splitted_libraries">
            <ns4:name>Splitted libraries</ns4:name>
            <ns4:shortDescription>Splitted library input for QIIME</ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>FILE</ns4:handling>
            <ns4:option>-i </ns4:option>
        </ns4:input>
        <ns4:input id="input_parameter_file">
            <ns4:name>Parameter file</ns4:name>
            <ns4:shortDescription></ns4:shortDescription>
            <ns4:description>
                Path to the parameter file, which specifies changes to
                the default behavior. See
                http://www.qiime.org/documentation/file_formats.html
                #qiime-parameters . [if omitted, default values will
                be used]
            </ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>FILE</ns4:handling>
            <ns4:option>-p </ns4:option>
        </ns4:input>
        <ns4:input id="input_reference_sequences">
            <ns4:name>Reference sequences</ns4:name>
            <ns4:shortDescription></ns4:shortDescription>
            <ns4:description>
                the reference sequences [default:
                /usr/local/lib/python2.7/site-packages/qiime_default_r
                eference/gg_13_8_otus/rep_set/97_otus.fasta]
            </ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>FILE</ns4:handling>
            <ns4:option>-r </ns4:option>
        </ns4:input>
        
        
        <ns4:output id="PLACEHOLDER_output_open_reference_otus_directory">
            <ns4:name>Placeholder output</ns4:name>
            <ns4:shortDescription>Placeholder output</ns4:shortDescription>
            <ns4:description>Placeholder ... which is not really existent and will be hadled via a static commandline argument</ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>DIRECTORY</ns4:handling>
        </ns4:output>
        <ns4:output id="output_open_reference_otus_directory">
            <ns4:name>QIIME ouput directory</ns4:name>
            <ns4:shortDescription>Output dir for QIIME.</ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type>ToolDependentRepresentation</ns4:type>
            <ns4:handling>DIRECTORY</ns4:handling>
            <ns4:option>-o </ns4:option>
        </ns4:output>
        
        
        <ns4:outputfile id="rep_set_tre">
            <ns4:name>Rep Set</ns4:name>
            <ns4:filename>rep_set.tre</ns4:filename>
            <ns4:contenttype>plain/text</ns4:contenttype>
        </ns4:outputfile>
        <ns4:outputfile id="otu_table_mc2_w_tax_no_pynast_failures_biom">
            <ns4:name>otu table mc2 w tax no pynast failures biom</ns4:name>
            <ns4:filename>otu_table_mc2_w_tax_no_pynast_failures.biom</ns4:filename>
            <ns4:contenttype>plain/text</ns4:contenttype>
        </ns4:outputfile>
        
        <!-- 
            There are a lot of other output files which are not used in the
            16S pipeline. Still, they should be included in the near future 
            so that the tool is feature complete. 
        -->
        
        
        <ns4:param id="jobs_to_start">
            <ns4:name>Jobs to start</ns4:name>
            <ns4:shortDescription>Number of jobs to start.</ns4:shortDescription>
            <ns4:description>
                Number of jobs to start. NOTE: you must also pass -a
                        to run in parallel, this defines the number of jobs to
                        be started if and only if -a is passed [default: 1]
            </ns4:description>
            <ns4:type>int</ns4:type>
            <ns4:option>-O </ns4:option>
            <ns4:guiElement>INPUTTEXT</ns4:guiElement>
        </ns4:param>
        <ns4:param id="otu_picking_method">
            <ns4:name>OTU picking method</ns4:name>
            <ns4:shortDescription>Method for picking OTUs</ns4:shortDescription>
            <ns4:description>
                The OTU picking method to use for reference and de
                        novo steps. Passing usearch61, for example, means that
                        usearch61 will be used for the de novo steps and
                        usearch61_ref will be used for reference steps.
                        [default: uclust]
            </ns4:description>
            <ns4:type>string</ns4:type>
            <ns4:option>-m </ns4:option>
            <ns4:defaultValue>uclust</ns4:defaultValue>
            <ns4:guiElement>INPUTTEXT</ns4:guiElement>
        </ns4:param>
        <ns4:param id="parallel">
            <ns4:name>Run in parallel</ns4:name>
            <ns4:shortDescription>Whether or not parallel computation should be used</ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type>boolean</ns4:type>
            <ns4:option>-a </ns4:option>
            <ns4:guiElement>SELECTBOOLEANCHECKBOX</ns4:guiElement>
        </ns4:param>
        <ns4:param id="verbose">
            <ns4:name>Use verbose output</ns4:name>
            <ns4:shortDescription>Whether or not the verbosity flag should be used</ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type>boolean</ns4:type>
            <ns4:option>-v </ns4:option>
            <ns4:guiElement>SELECTBOOLEANCHECKBOX</ns4:guiElement>
        </ns4:param>
        <ns4:param id="suppress_step_four">
            <ns4:name>Suppress step 4</ns4:name>
            <ns4:shortDescription>Whether or not step 4 should be suppressed</ns4:shortDescription>
            <ns4:description></ns4:description>
            <ns4:type>boolean</ns4:type>
            <ns4:option>--suppress_step4 </ns4:option>
            <ns4:guiElement>SELECTBOOLEANCHECKBOX</ns4:guiElement>
        </ns4:param>
        
        
        <ns4:function id="function_qiime_pick_open_reference_otus">
            <ns4:shortDescription>Performs a fast quality control on the FASTQ file</ns4:shortDescription>
            <ns4:inputref ref="input_splitted_libraries"/>
            <ns4:inputref ref="input_parameter_file"/>
            <ns4:inputref ref="input_reference_sequences"/>
            <ns4:outputref ref="PLACEHOLDER_output_open_reference_otus_directory"/>
            <ns4:outputfileref ref="rep_set_tre"/>
            <ns4:outputfileref ref="otu_table_mc2_w_tax_no_pynast_failures_biom"/>
            
            <ns4:paramGroup id="qiime_demultiplex_params">
                <ns4:paramref ref="jobs_to_start"/>
                <ns4:paramref ref="otu_picking_method"/>
                <ns4:paramref ref="parallel"/>
                <ns4:paramref ref="suppress_step_four"/>
                <ns4:paramref ref="verbose"/>
            </ns4:paramGroup>
            
            <ns4:paramAndInputOutputOrder>
                <ns4:reference>input_splitted_libraries</ns4:reference>
                <ns4:reference>parallel</ns4:reference>
                <ns4:reference>jobs_to_start</ns4:reference>
                <ns4:reference>otu_picking_method</ns4:reference>
                <ns4:reference>verbose</ns4:reference>
                <ns4:reference>input_parameter_file</ns4:reference>
                <ns4:reference>input_reference_sequences</ns4:reference>
                <ns4:reference>suppress_step_four</ns4:reference>
                <!-- 
                     the latter two are for the case that the placeholder output 
                     is used. Otherwise use the other output ... 
                -->
                <ns4:additionalString>-f -o /var/spool/cwl</ns4:additionalString>
                <ns4:reference>PLACEHOLDER_output_open_reference_otus_directory</ns4:reference>
            </ns4:paramAndInputOutputOrder>
        </ns4:function>
        
        
    </ns4:executable>
    
    
    
    
    <ns4:view type="submission" id="view_submission">
        <ns4:title>QIIME pick open reference OTUs submission</ns4:title>
        <ns4:customContent xml:lang="en"/>
    </ns4:view>
    
    <ns4:view type="webservice" id="view_webservice">
        <ns4:title xml:lang="en">QIIME pick open reference OTUs WebService</ns4:title>
        <ns4:customContent xml:lang="en"/>
    </ns4:view>
    
    
    
</ns4:runnableItem>